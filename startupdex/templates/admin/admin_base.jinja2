<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">
<html lang="en">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    {% block head %}
    <title>{% block title %}{% endblock %}StartupDex</title>
		<link href="{{ gibs.static_url }}css/bootstrap.css" rel="stylesheet" media="all">
		{#<link href="{{ gibs.static_url }}css/bootstrapstyle.css" rel="stylesheet" media="all">#}
		<link href="{{ gibs.static_url }}css/style.css" rel="stylesheet" media="all">
    {% endblock %}
</head>
<body>
	<div class="navbar navbar-default navbar-static-top">
		<div class="container">
			<div class="navbar-header">
				<a href="/" class="navbar-brand">
					StartupDex
				</a>

				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
					<span class="sr-only">
						Toggle Navigation
					</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
			</div>

			<div class="collapse navbar-collapse">
				<ul class="nav navbar-nav navbar-left">
					<!--TODO: add active classes and hide items depending on if logged_in-->
					<li><a href="{{ gibs.application_url }}browse">Browse Startups</a></li>
					<li>
						<a href="about" data-target="#" data-toggle="dropdown">About <span class="caret"></span></a>
						<ul class="dropdown-menu">
							<li><a href="#">History</a></li>
							<li><a href="#">Contact Us</a></li>
							<li><a href="#">Blog</a></li>
							<li class="divider"></li>
							<li><a href="#">Facebook</a></li>
							<li class="divider"></li>
							<li><a href="#">Twitter</a></li>
						</ul>
					</li>
					<li><a href="#">Take a Tour</a></li>
				</ul>
				
				<form class="navbar-form navbar-right" role="search">
					<div class="form-group">
						<div class="input-group">
							<input class="form-control" type="text" placeholder="Search">
							<div class="input-group-btn">
								<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
							</div>
						</div>
					</div>
				</form>

				<ul class="nav navbar-nav navbar-right">
					<li><a href="#">Login</a></li>
				
				</ul>
			</div>

		</div>

	</div>


	<div class="container">
		<div class="row">
			<div class="col-md-9">
				{% block left_focus %}{% endblock left_focus %}
			</div>

			<div class="col-md-3">
				{% block right_focus %}
				admin toolbar
				
				
				{% endblock right_focus %}
			</div>
		</div>
	</div>

	<br />
	<br />
	<br />

	<div class="container footer">
		{% block footer %}
		<p>&copy; Copyright 2008 by <a href="http://domain.invalid/">you</a>.</p>
		{% endblock %}
	</div>


	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
	<script src="{{ gibs.static_url }}js/admin.js"></script>
	<script src="{{ gibs.static_url }}js/bootstrap.js"></script>
</body>

