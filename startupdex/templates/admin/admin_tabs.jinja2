
{% set navigation_bar  = [
	('admin/frontpage', 'frontpage', 'Manage Frontpage'),
	('admin/db_angelco', 'db_angelco', 'Manage angel.co data'),
	('admin/db_users', 'db_users', 'Manage users'),
] -%}

<div role="tabpanel">
	<ul class="nav nav-tabs" role="tablist">
		{% for href, page_id, caption in navigation_bar %}
		<li role="presentation" {% if page_id == active_tab %} class="active" {% endif %}><a href="{{ gibs.application_url }}{{ href|e }}" role="tab">{{ caption|e }}</a></li>
		{% endfor %}
	</ul>
</div>

